#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n0=0,n1=0,n2=0,n3=0,n4=0,n5=0,n6=0,n7=0,n8=0,n9=0;
 int num;
    scanf("%d",&num);
    
int temp=0,rem,dig,i;
   


while (num !=0){
  
  rem=num%10;
  num=num/10;
temp=0;
temp +=rem;
    if(temp==0){
        
        n0 +=1;
    }
    else if(temp==1){
        n1 +=1;
    }
    else if(temp==2){
        n2 +=1;
    }
    else if(temp==3){
        n3 +=1;
    }
    else if(temp==4){
        n4 +=1;
    }
    else if(temp==5){
        n5 +=1;
    }
    else if(temp==6){
        n6 +=1;
    }
    else if(temp==7){
        n7 +=1;
    }
    else if(temp==8){
        n8 +=1;
    }
    else if(temp==9){
        n9 +=1;
    }
    
  
}
    printf("%d %d %d %d %d %d %d %d %d %d",n0,n1,n2,n3,n4,n5,n6,n7,n8,n9);
    
    
    return 0;
}
